<Application x:Class="Dawdle.Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/Base.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Brushes.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Buttons.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Icons.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Sliders.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Thumbs.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
